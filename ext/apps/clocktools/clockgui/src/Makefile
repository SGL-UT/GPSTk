LFLAGS   =
LIBS     = $(SUBLIBS) -L$(QTDIR)/lib -L/usr/X11R6/lib64 -L/usr/X11R6/lib64 -lqt-mt -lGLU -lGL -lXmu -lXext -lX11 -lm
AR       = ar cqs
RANLIB   =
MOC      = $(QTDIR)/bin/moc
UIC      = $(QTDIR)/bin/uic
QMAKE    = qmake
TAR      = tar -cf
GZIP     = gzip -9f
COPY     = cp -f
COPY_FILE= $(COPY)
COPY_DIR = $(COPY) -r
DEL_FILE = rm -f
SYMLINK  = ln -sf
DEL_DIR  = rmdir
